{
  "extends": "@dcl/eslint-config/sdk",
  "parserOptions": {
    "project": ["tsconfig.json"]
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": true,
        "tabWidth": 2,
        "endOfLine": false
      }
    ]
  }
}
